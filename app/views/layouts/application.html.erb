<!DOCTYPE html>
<html>
  <head>
    <title>English</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'angular.js', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag asset_path('favicon.ico'), :rel => 'icon', :type => "image/x-icon" %>
    <%= javascript_include_tag 'diff-match.js', 'data-turbolinks-track': 'reload' %>
    
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    <script src='/assets/recaptcha.js' type="text/javascript"></script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
